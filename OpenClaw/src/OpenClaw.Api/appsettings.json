{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "PostgreSql",
    "ConnectionString": "Host=localhost;Database=weda_template;Username=postgres;Password=postgres",
    "DatabaseName": "WedaTemplate"
  },
  "Authentication": {
    "Enabled": true
  },
  "JwtSettings": {
    "Secret": "a-very-super-secret-key-that-is-long-enough",
    "TokenExpirationInMinutes": 0,
    "Issuer": "WedaTemplate",
    "Audience": "WedaTemplate"
  },
  "EmailSettings": {
    "EnableEmailNotifications": false,
    "DefaultFromEmail": "",
    "SmtpSettings": {
      "Server": "",
      "Port": 0,
      "Username": "",
      "Password": ""
    }
  },
  "OpenTelemetry": {
    "Enabled": true,
    "ServiceName": "OpenClaw",
    "ServiceVersion": "1.0.0",
    "Dashboard": "Aspire",
    "OtlpEndpoint": "http://localhost:4317"
  }
}